{
	"name": "copy-with-line-number",
	"displayName": "Copy with Line Number",
	"description": "",
	"version": "1.0.0",
	"publisher": "kota-kota",
	"repository": {
		"type": "git",
		"url": "https://github.com/kota-kota/vscode-extensions.git"
	},
	"engines": {
		"vscode": "^1.40.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:copy-with-line-number.copy"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "copy-with-line-number.copy",
				"title": "Copy with Line Number: Copy"
			}
		],
		"keybindings": [
			{
				"command": "copy-with-line-number.copy",
				"key": "ctrl+alt+c"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.40.0",
		"@types/glob": "^7.1.4",
		"@types/mocha": "^9.0.0",
		"@types/node": "14.x",
		"@typescript-eslint/eslint-plugin": "^4.31.1",
		"@typescript-eslint/parser": "^4.31.1",
		"eslint": "^7.32.0",
		"glob": "^7.1.7",
		"mocha": "^9.1.1",
		"typescript": "^4.4.3",
		"@vscode/test-electron": "^1.6.2"
	}
}
